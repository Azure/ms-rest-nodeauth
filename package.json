{
  "name": "ms-rest-nodeauth",
  "author": {
    "name": "Microsoft Corporation",
    "email": "azsdkteam@microsoft.com",
    "url": "https://github.com/Azure/ms-rest-nodeauth"
  },
  "version": "0.5.0",
  "description": "Azure Authentication library in node.js with type definitions.",
  "tags": [
    "node",
    "isomorphic",
    "browser",
    "azure",
    "autorest",
    "authentication",
    "environment"
  ],
  "keywords": [
    "node",
    "isomorphic",
    "browser",
    "azure",
    "autorest",
    "authentication",
    "environment"
  ],
  "main": "./dist/lib/msRestNodeAuth.js",
  "types": "./typings/lib/msRestNodeAuth.d.ts",
  "dependencies": {
    "adal-node": "^0.1.22",
    "ms-rest-azure-env": "^0.1.1",
    "ms-rest-js": "~0.19.379",
    "ts-node": "^7.0.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^2.2.40",
    "@types/should": "^8.1.30",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "should": "5.2.0",
    "tslint": "^5.2.0",
    "typescript": "^2.5.2"
  },
  "homepage": "https://github.com/Azure/ms-rest-nodeauth",
  "repository": {
    "type": "git",
    "url": "git@github.com:Azure/ms-rest-js.git"
  },
  "bugs": {
    "url": "http://github.com/Azure/ms-rest-nodeauth/issues"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "lib/**/*.ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "test": "run-p test:tslint test:unit",
    "test:unit": "nyc mocha",
    "test:tslint": "tslint -p . -c tslint.json --exclude test/**/*.ts",
    "unit": "mocha -t 50000 dist/test",
    "build": "npm -s run-script tsc",
    "tslint": "tslint -p . -c tslint.json --exclude test/**/*.ts"
  }
}
